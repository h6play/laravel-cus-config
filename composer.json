{
    "name": "laravel-cus-config",
    "description": "Laravel-cus-config extension for laravel",
    "type": "composer-plugin",
    "keywords": ["Laravel-cus-config", "extension", "config"],
    "homepage": "https://github.com/h6play/laravel-cus-config",
    "license": "MIT",
    "authors": [
        {
            "name": "h6play",
            "email": "h6play@163.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.5|~6.0"
    },
    "autoload": {
        "psr-4": {
            "H6play\\LaravelCusConfig\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "H6play\\LaravelCusConfig\\CusConfigServiceProvider"
            ]
        }
    }
}
